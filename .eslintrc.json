{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console":"off",
        "linebreak-style":"off",
        "no-restricted-imports":"off",
        "max-len":"off",
        "no-shadow":"off",
        "consistent-return":"off"
    }
}
